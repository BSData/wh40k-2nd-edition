        <profile id="6b9e-2e09-7df0-3e4f" name="Maximum power" publicationId="06e5-8e36-e8c8-aec3" page="37" hidden="false" typeId="d8e8-6c66-f558-30ee" typeName="Weapon">
          <characteristics>
            <characteristic name="Short range" typeId="a108-ac67-8c42-6bbb">20</characteristic>
