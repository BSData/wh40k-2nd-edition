                <profile id="8fcb-2666-61a3-badd" name="Foulspawn" publicationId="02e1-3f69-7ae7-f5c5" page="140" hidden="false" typeId="219f-0faa-a2d8-f766" typeName="Unit">
                  <characteristics>
                    <characteristic name="Unit Type" typeId="49de-956f-8797-7436">Foulspawn</characteristic>
                    <characteristic name="M" typeId="4934-7872-3dd7-a8b6">2D6</characteristic>
                    <characteristic name="WS" typeId="dc18-b600-96a9-a4a3">3</characteristic>
