              <profiles>
                <profile id="f65e-9ff6-58b2-b6d5" name="Psycho-Sonic Screamers" hidden="false" typeId="cc8b-49d5-829c-bfaf" typeName="Wargear">
                  <characteristics>
                    <characteristic name="Special" typeId="f917-8cd8-7592-47dd">Vehicle causes Terror</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <infoLinks>
