                        <condition field="selections" scope="parent" value="0.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" childId="ce9c-8d5f-4951-2427" type="notInstanceOf"/>
                        <condition field="selections" scope="parent" value="0.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" childId="5943-bd5c-e0a1-518a" type="notInstanceOf"/>
                      </conditions>
