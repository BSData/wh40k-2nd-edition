    <profile id="083f-467a-f979-1721" name="Commissar" publicationId="a1e3-886b-e509-d15a" page="20" hidden="false" typeId="219f-0faa-a2d8-f766" typeName="Unit">
      <characteristics>
        <characteristic name="Unit Type" typeId="49de-956f-8797-7436">Commissar</characteristic>
