    <selectionEntry id="9670-cbf6-92b2-96fe" name="Cypher, Fallen Dark Angel Champion" publicationId="02e1-3f69-7ae7-f5c5" page="143" hidden="false" collective="false" import="true" type="upgrade">
      <profiles>
        <profile id="f139-6579-395e-40a0" name="Cypher" hidden="false" typeId="219f-0faa-a2d8-f766" typeName="Unit">
          <characteristics>
            <characteristic name="Unit Type" typeId="49de-956f-8797-7436">Cypher</characteristic>
